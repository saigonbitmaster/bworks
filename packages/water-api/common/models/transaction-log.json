{
  "name": "TransactionLog",
  "base": "BaseLog",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "generated": false,
      "required": true
    },
    "clientId": {
      "type": "objectid",
      "required": true
    },
    "meterNumberId": {
      "type": "string",
      "required": true
    },
    "extra": {
      "type": "object"
    },
    "returned": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "service": {
      "type": "string",
      "required": true,
      "enum": [
        "MOMO",
        "VNPAY"
      ]
    },
    "result": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
