{
  "name": "MeterNumberSubmit",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "generated": false,
      "required": true
    },
    "currentNumber": {
      "type": "number",
      "required": true
    },
    "submitDate": {
      "type": "date"
    },
    "createDate": {
      "type": "date",
      "defaultFn": "now"
    },
    "meterImage": {
      "type": "array"
    },
    "fromDate": {
      "type": "date"
    },
    "toDate": {
      "type": "date"
    },
    "submitStatus": {
      "type": "string",
      "enum": [
        "PASSED",
        "DENIED",
        "WAITING",
        "EXISTED"
      ],
      "default": "WAITING"
    },
    "termMonth": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "clientId"
    }
  },
  "acls": [],
  "methods": {}
}
