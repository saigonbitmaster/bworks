{
  "name": "WaterProvider",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "importKey": "name",
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "address": {
      "type": "string",
      "required": true
    },
    "providerType": {
      "type": "string",
      "enum": [
        "1",
        "2",
        "3"
      ],
      "default": "1",
      "max": 1
    },
    "taxNo": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "fax": {
      "type": "number"
    },
    "email": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "contactPerson": {
      "type": "string"
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "rootMeters": {
      "type": "hasMany",
      "model": "RootMeter",
      "foreignKey": ""
    },
    "dma": {
      "type": "referencesMany",
      "model": "dma",
      "foreignKey": ""
    },
    "clients": {
      "type": "hasMany",
      "model": "Client",
      "foreignKey": "providerId"
    },
    "bankAccounts": {
      "type": "embedsMany",
      "model": "BankAccount",
      "property": "bankAccountList",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "geoward": {
      "type": "referencesMany",
      "model": "GeoWard",
      "foreignKey": "wardIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}