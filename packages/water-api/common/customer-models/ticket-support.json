{
  "name": "TicketSupport",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "createdDate": {
      "type": "date",
      "required": true
    },
    "isClosed": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "isAdmin": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "masterBody": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "ticketType": {
      "type": "belongsTo",
      "model": "TicketType",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "ticketPriority": {
      "type": "belongsTo",
      "model": "TicketPriority",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "appUser": {
      "type": "belongsTo",
      "model": "AppUser",
      "foreignKey": "receiverId",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "clientUser": {
      "type": "belongsTo",
      "model": "ClientUser",
      "foreignKey": "clientUserId",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "ticketBody": {
      "type": "hasMany",
      "model": "TicketBody",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
