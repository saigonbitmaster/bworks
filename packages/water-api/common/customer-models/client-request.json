{
  "name": "ClientRequest",
  "base": "BaseModel",
  "idInjection": true,
  "forceId": false,
  "strict": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "require": true
    },
    "startMeterNumber": {
      "type":"number",
      "index": true
    },
    "oldMeterNumber": {
      "type":"number",
      "index": true
    },
    "newMeterNumber": {
      "type":"number",
      "index": true
    },
    "setupDate": {
      "type":"date",
      "index": true
    },
    "type": {
      "type": "string",
      "enum": [
        "NEW_INSTALL",
        "REPLACE"
      ],
      "default": "NEW_INSTALL",
      "require": true
    },
    "status": {
      "type": "string",
      "enum": [
        "NEW",
        "INPROCESS",
        "PENDING",
        "COMPLETE"
      ],
      "default": "NEW",
      "require": true
    },
    "description": {
      "type": "string"
    },
    "qrCode": {
      "type": "string",
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "clientId"
    },
    "installationTeam": {
      "type": "belongsTo",
      "model": "InstallationTeam",
      "foreignKey": "installationTeamId"
    }
  },
  "acls": [],
  "methods": {}
}
