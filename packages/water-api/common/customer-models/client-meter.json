{
  "name": "ClientMeter",
  "base": "BaseModel",
  "idInjection": true,
  "strict": true,
  "options": {
    "validateUpsert": true
  },
  "indexes": {
    "fullText": {
      "keys": {
        "qrCode": "text"
      },
      "options": {}
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string"
    },
    "serial": {
      "type": "string"
    },
    "size": {
      "type": "number"
    },
    "inUse": {
      "type": "boolean"
    },
    "testedDate": {
      "type": "date"
    },
    "setupDate": {
      "type": "date"
    },
    "startMeterDate": {
      "type": "date"
    },
    "status": {
      "type": "string",
      "default": "OK"
    },
    "contractDate": {
      "type": "date"
    },
    "startMeterNumber": {
      "type": "number"
    },
    "qrCode": {
      "type": "string",
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "clientId",
      "index": true
    },
    "rootMeter": {
      "type": "belongsTo",
      "model": "RootMeter",
      "foreignKey": "rootMeterId"
    },
    "installTeam": {
      "type": "belongsTo",
      "model": "InstallationTeam",
      "foreignKey": "installationTeamId"
    },
    "provider": {
      "type": "belongsTo",
      "model": "WaterProvider",
      "foreignKey": "providerId"
    }
  },
  "acls": [],
  "methods": {}
}
