{
  "name": "ClientMeterHistory",
  "base": "BaseModel",
  "idInjection": true,
  "strict": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "lastMeterNumber": {
      "type": "number",
      "description": "Số nước của đầu kỳ thay đồng hồ'"
    },
    "oldMeterNumber": {
      "type": "number",
      "require": true
    },
    "newMeterNumber": {
      "type": "number",
      "require": true
    },
    "type": {
      "type": "string",
      "required": "true",
      "default": "NEW_INSTALL",
      "enum": ["NEW_INSTALL", "REPLACE"]
    },
    "setupDate": {
      "type": "date",
      "require": true
    }
  },
  "validations": [],
  "relations": {
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "clientId"
    },
    "meter": {
      "type": "belongsTo",
      "model": "ClientMeter",
      "foreignKey": "meterId"
    }
  },
  "acls": [],
  "methods": {}
}
