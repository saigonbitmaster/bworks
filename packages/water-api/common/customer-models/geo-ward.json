{
  "name": "GeoWard",
  "base": "BaseModel",
  "idInjection": true,
  "forceId": false,
  "options": {
    "validateUpsert": true
  },
  "indexes": {
    "fullText": {
      "keys": {
        "fullName": "text",
        "code": "text"
      },
      "options": {
        "weights": {
          "fullName": 10,
          "code": 8
        }
      }
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "prefix": {
      "type": "string",
      "required": true
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "fullAddress": {
      "type": "string"
    },
    "code": {
      "type": "string",
      "required": true
    },
    "population": {
      "type": "number"
    },
    "position": {
      "type": "geopoint",
      "index": true
    },
    "assignedToGeoGroup": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "geoCountry": {
      "type": "belongsTo",
      "model": "GeoCountry",
      "foreignKey": "countryId",
      "options": {
        "nestRemoting": true
      }
    },
    "geoProvince": {
      "type": "belongsTo",
      "model": "GeoProvince",
      "foreignKey": "provinceId",
      "options": {
        "nestRemoting": true
      }
    },
    "geoDistrict": {
      "type": "belongsTo",
      "model": "GeoDistrict",
      "foreignKey": "districtId",
      "options": {
        "nestRemoting": true
      }
    },
    "geoGroup": {
      "type": "belongsTo",
      "model": "GeoGroup",
      "foreignKey": "geoGroupId",
      "options": {
        "nestRemoting": true
      }
    },
    "gadm": {
      "type": "belongsTo",
      "model": "Gadm"
    }
  },
  "acls": [],
  "methods": {}
}
