{
  "name": "WaterSourceGroup",
  "base": "BaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "strict": true,
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "max": 64
    },
    "type": {
      "type": "string",
      "enum": ["ACTIVE", "BACKUP", "RESERVED"]
    },
    "estimatedReserves": {
      "type": "number",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "boundary": {
      "type": [
        "object"
      ]
    }
  },
  "validations": [],
  "relations": {
    "province": {
      "type": "belongsTo",
      "model": "GeoProvince"
    },
    "district": {
      "type": "belongsTo",
      "model": "GeoDistrict"
    },
    "ward": {
      "type": "belongsTo",
      "model": "GeoWard"
    },
    "quarter": {
      "type": "belongsTo",
      "model": "GeoQuarter"
    },
    "waterSource": {
      "type": "hasMany",
      "model": "WaterSource"
    }
  },
  "acls": [],
  "methods": {}
}