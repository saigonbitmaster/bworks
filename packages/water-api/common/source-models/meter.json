{
  "name": "Meter",
  "base": "PersistedModel",
  "idInjection": true,
  "forceId": false,
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "string",
      "required": true
    },
    "accuracy": {
      "type": "string"
    },
    "flowRate": {
      "type": "string"
    },
    "manufacturedDate": {
      "type": "date"
    },
    "setupDate": {
      "type": "date",
      "required": true
    },
    "operatingTemperature": {
      "type": "number"
    },
    "operatingPressure": {
      "type": "number"
    },
    "environmentalRating": {
      "type": "string",
      "required": true,
      "default": "IP68",
      "enum": [
        "IP65",
        "IP66",
        "IP67",
        "IP68",
        "IP69"
      ]
    },
    "materialStatus": {
      "type": "string",
      "enum": [
        "1",
        "2",
        "3",
        "4"
      ]
    },
    "position": {
      "type": "geopoint",
      "index": true
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "dataLogger": {
      "type": "belongsTo",
      "model": "DataLogger",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "waterSource": {
      "type": "belongsTo",
      "model": "WaterSource",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}