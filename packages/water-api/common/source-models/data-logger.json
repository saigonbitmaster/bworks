{
  "name": "DataLogger",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "readDataRate": {
      "type": "string",
      "required": true
    },
    "sendDataRate": {
      "type": "string",
      "required": true
    },
    "memoryStorage": {
      "type": "string",
      "required": true
    },
    "manufacturedDate": {
      "type": "date"
    },
    "setupDate": {
      "type": "date",
      "required": true
    },
    "operatingTemperature": {
      "type": "number"
    },
    "operatingPressure": {
      "type": "number"
    },
    "environmentalRating": {
      "type": "string",
      "required": true,
      "default": "IP68",
      "enum": [
        "IP65",
        "IP66",
        "IP67",
        "IP68",
        "IP69"
      ]
    },
    "powerSource": {
      "type": "array",
      "required": true
    },
    "batteryLife": {
      "type": "number"
    },
    "lastChargeBattery": {
      "type": "date"
    },
    "materialStatus": {
      "type": "string",
      "enum": [
        "1",
        "2",
        "3",
        "4"
      ]
    },
    "location": {
      "type": "geopoint",
      "index": true
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "waterSource": {
      "type": "belongsTo",
      "model": "WaterSource"
    }
  },
  "acls": [],
  "methods": {}
}